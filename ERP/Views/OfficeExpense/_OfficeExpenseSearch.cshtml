<div class="container col-md-12 col-lg-12 width-100">
    <div class="col-md-12 col-lg-12">
        <div class="col-md-6 col-lg-6 commonPadding">
            <span class="BodyWelcomeText">Search Options</span>
        </div>
    </div>
    <div class="col-md-12 col-lg-12">

        <div class="col-md-6 col-lg-6 commonPadding">
            <input type="text" placeholder="Search By OfficeExpense Name" id="OfficeExpenseName" name="OfficeExpenseName" class="textboxstyles searchText" style="width:300px;height:27px;" />
            <input type="hidden" name="OfficeExpenseID" id="OfficeExpenseID" />

        </div>
    </div>
    <div class="col-md-12 col-lg-12">
        <div class="col-md-4 col-lg-4 commonPadding textleft">
            <input type="button" value="Search" data-url="@Url.Action("OfficeExpenseSearch", "OfficeExpense")" class="formbutton OfficeExpenseSearch" />
        </div>
    </div>
    <div class="col-md-12 col-lg-12 commonPadding" style="text-align:right">
        <input type="button" value="Add Office Expense" data-url="@Url.Action("_OfficeExpenseEdit", "OfficeExpense")" data-identity="-1" class="formbutton OfficeExpenseAdd" />
    </div>

</div>

